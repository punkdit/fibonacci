	Eprint = {0810.1983},
	Journal = {New J. Phys.},
	Number = {4},
	Pages = {043029},
	Title = {A no-go theorem for a two-dimensional self-correcting quantum memory based on stabilizer codes},
	Volume = {11},
	Year = {2009},
	Bdsk-File-1 = {},
